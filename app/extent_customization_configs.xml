<documentTitle>ZCommerce</documentTitle>
<!-- <!– protocol for script and stylesheets–> -->
<!-- <!- defaults to https–> -->
<protocol>http</protocol>
<!-- <!–document encoding –>  -->
<!-- <!–defaults to UTF-8 –>  -->
